---
import { getCollection } from 'astro:content';
import Page from '../../layouts/Page.astro';
import ProjectItem from '../../components/ProjectItem.astro';
import { SITE_TITLE } from '../../consts';

const projects = [
  {
    title: 'Duofolio',
    description:
      'An ebook reader to help language learners effectively acquire new vocabulary in their target language',
    link: 'https://github.com/farshed/duofolio'
  },
  {
    title: 'SoundSpice',
    description:
      'A light-weight and minimalist music player for Android. Featured by Beebom as one of the best open-source Android apps of 2020. It had in-app lyrics years before Spotify.',
    link: 'https://github.com/farshed/soundspice-mobile'
  },
  {
    title: 'genius-lyrics-api',
    description: 'A library for scraping song lyrics and other metadata from genius.com.',
    link: 'https://github.com/farshed/genius-lyrics-api'
  },
  {
    title: 'pennywise',
    description: 'A production-grade banking system',
    link: 'https://github.com/farshed/pennywise'
  },
  {
    title: 'zusammen',
    description: 'A CLI-based parallel downloader written in Rust.',
    link: 'https://github.com/farshed/zusammen'
  },
  {
    title: 'DeepGaze',
    description: 'A CLI-based parallel downloader written in Rust.',
    link: 'https://github.com/farshed/zusammen'
  }
];
---

<Page>
  <ul class="post-list">
    {projects.map((p) => <ProjectItem {...p} />)}
  </ul>
</Page>
