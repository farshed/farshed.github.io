          <a href="mailto:mail@farshed.me">mail@farshed.me</a>
          <p class="text-gray-400">|</p>
          <a href="tel:+92-3096855375">(+92) 309 6855375</a>
        </div>
      </div>

      <!-- Main Content -->
      <div class="px-4 pt-2">
        <!-- Summary Section -->
