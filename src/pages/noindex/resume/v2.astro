                  <div>
                    <h3 class="text-sm font-semibold">{exp.role}</h3>
                    <p>{exp.company}</p>
                  </div>
                  <div class="text-right">
                    <p class="font-medium">{exp.date}</p>
                  </div>
                </div>
                <ul class="list-disc ps-3 space-y-0.5">
                  {exp.keypoints.map((kp) => (
                    <li>{kp}</li>
                  ))}
                </ul>
