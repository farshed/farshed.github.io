---
const { title, description, techStack, links = {} } = Astro.props;
---

<li class="project-item">
  <h2>{title}</h2>
  <p>{description}</p>
  {techStack && <p>Tech stack: {techStack.join(', ')}</p>}
  <div class="links">
    {
      Object.entries(links).map(([key, value]) => (
        <a href={value as string} target="_blank">
          {key}
        </a>
      ))
    }
  </div>
</li>
